{
    "manifest_version": 3,
    "name": "Productivity Tracker",
    "version": "1.0.0",
    "description": "Tracks time spent on different websites.",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "notifications"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/pie-chart-16.png",
            "48": "icons/pie-chart-48.png",
            "128": "icons/pie-chart-128.png"
        }
    },
    "icons": {
        "16": "icons/pie-chart-16.png",
        "48": "icons/pie-chart-48.png",
        "128": "icons/pie-chart-128.png"
    }
}